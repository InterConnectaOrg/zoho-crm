{
    "name": "interconnecta/zoho-crm",
    "keywords": ["zoho", "crm", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Gustavo Levano",
            "email": "emw3wme@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "laravel/framework": "~5.8.0|^6.0",
        "zohocrm/php-sdk": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Zoho\\CRM\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zoho\\CRM\\ZohoCRMServiceProvider"
            ],
            "aliases": {
                "ZohoCRM": "Zoho\\CRM\\Facades\\ZohoCRM"
            }
        }
    },
    "minimum-stability": "dev"
}
